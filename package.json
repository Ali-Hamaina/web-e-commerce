{
  "name": "yuna-parfum-monorepo",
  "version": "1.0.0",
  "description": "Yuna Parfum E-commerce Platform - Monorepo for Frontend and Backend",
  "private": true,
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "dev:backend": "cd backend && nodemon server.js",
    "dev:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "test": "echo \"Running tests...\" && cd frontend && npm test"
  },
  "keywords": [
    "ecommerce",
    "parfum",
    "react",
    "node.js",
    "mongodb",
    "typescript"
  ],
  "author": "Yuna Parfum",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0"
  }
}
